@model WebRedisEx.ViewModels.UserVM

<form asp-action="Create" asp-controller="User" method="post">
    <div class="row">
        <div class="col-2">
            <label asp-for="UserName"></label>
        </div>
        <div class="col-10">
            <input asp-for="UserName" />
        </div>
    </div>
    <div class="row">
        <div class="col-2">
            <label asp-for="Age"></label>
        </div>
        <div class="col-10">
            <input asp-for="Age" />
        </div>
    </div>
    <button type="submit">Submit</button>
</form>